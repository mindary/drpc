{
  "name": "@remly/ws",
  "description": "The remly WebSocket transport",
  "version": "0.1.0",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
