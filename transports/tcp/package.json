{
  "name": "@drpc/transport-tcp",
  "description": "The TCP transport",
  "version": "0.1.0",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
