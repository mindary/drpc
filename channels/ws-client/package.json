{
  "name": "@remly/ws-client",
  "description": "The remly websocket client library",
  "version": "0.4.1",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "Yuan Tao",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean remly-ws-client*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs  \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha --allow-console-logs  \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf remly-ws-client*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "@remly/client": "^0.5.1",
    "@remly/core": "^0.12.1",
    "tily": "^2.8.3",
    "ts-essentials": "^7.0.3",
    "ws": "^8.1.0"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "@loopback/eslint-config": "^11.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/node": "^16.6.1",
    "@types/ws": "^7.4.7"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "browser": {
    "./dist/websocket.js": "./dist/websocket-browser.js"
  }
}
