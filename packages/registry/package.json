{
  "name": "@drpc/registry",
  "description": "A builtin service registry",
  "version": "0.2.0",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "Yuan Tao",
  "license": "MIT",
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean drpc-registry*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs  \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha --allow-console-logs  \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf drpc-registry*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "@drpc/core": "^0.15.0",
    "@drpc/decorators": "^0.2.0",
    "@drpc/types": "^0.5.0",
    "debug": "^4.3.2",
    "micromatch": "^4.0.4",
    "tily": "^2.8.3",
    "ts-essentials": "^8.1.0"
  },
  "devDependencies": {
    "@drpc/testlab": "^0.11.0",
    "@loopback/build": "^7.0.0",
    "@loopback/eslint-config": "^11.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/debug": "^4.1.7",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^16.4.1"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ]
}
