{
  "name": "@drpc/client",
  "description": "The drpc abstract client",
  "version": "0.8.0",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "Yuan Tao",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean drpc-client*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs  \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha --allow-console-logs  \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf drpc-client*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "@libit/interceptor": "^0.2.1",
    "@drpc/core": "^0.14.0",
    "@drpc/interception": "^0.3.4",
    "@drpc/resolver": "^0.1.0",
    "@drpc/transport-tcp": "^0.1.0",
    "@drpc/transport-ws": "^0.1.0",
    "debug": "^4.3.2",
    "once-promise": "^2.0.0",
    "parse-url": "^5.0.7",
    "tily": "^2.8.3",
    "ts-essentials": "^8.1.0",
    "ws": "^8.2.1"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "@loopback/eslint-config": "^11.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/debug": "^4.1.7",
    "@types/node": "^16.7.11",
    "@types/ws": "^7.4.7"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ]
}
