{
  "name": "@remly/tcp",
  "description": "The TCP broker for remly",
  "version": "0.1.0",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
