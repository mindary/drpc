    "tslib": "^2.0.2"
  },
  "devDependencies": {
    "@tib/build": "^6.3.4",
    "@tib/eslint-config": "^8.2.4",
    "@tib/testlab": "^3.4.4",
    "@types/node": "^14.11.5"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
