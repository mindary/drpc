{
  "name": "@drpc/benchmark",
  "description": "The drpc benchmark",
  "version": "0.1.1",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "author": "Yuan Tao",
  "license": "MIT",
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc -b",
    "clean": "lb-clean drpc-benchmark*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs  \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha --allow-console-logs  \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf drpc-benchmark*.tgz && tree package && npm run clean",
    "start": "node .",
    "prestart": "npm run clean && npm run build"
  },
  "dependencies": {
    "@caporal/core": "^2.0.2",
    "@drpc/client": "^0.10.0",
    "@drpc/connector-tcp": "^0.2.3",
    "@drpc/core": "^0.16.0",
    "@drpc/registry": "^0.3.0",
    "@drpc/server": "^0.15.0",
    "a-callback": "^0.1.1",
    "benny": "^3.6.15",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "poisson-process": "^1.0.0",
    "tily": "^2.8.3"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "@loopback/eslint-config": "^11.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/node": "^16.4.1"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ]
}
